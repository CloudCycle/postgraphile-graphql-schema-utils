{
  "name": "@cloudcycle/postgraphile-graphql-schema-utils",
  "version": "0.1.1",
  "description": "Utilities for extracting information from GraphQL schemas generated by Postgraphile",
  "main": "build/index.js",
  "scripts": {
    "clean": "npx rimraf ./build",
    "build": "npx tsc",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run clean && npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CloudCycle/postgraphile-graphql-schema-utils.git"
  },
  "keywords": [
    "Postgraphile",
    "GraphQL",
    "Schema",
    "Resolvers"
  ],
  "author": "Isac Casapu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CloudCycle/postgraphile-graphql-schema-utils/issues"
  },
  "homepage": "https://github.com/CloudCycle/postgraphile-graphql-schema-utils#readme",
  "devDependencies": {
    "graphql": "^15.4.0",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^7.5.5",
    "@graphql-tools/load": "^7.7.7"
  },
  "peerDependencies": {
    "graphql": ">=14.7"
  }
}
